    },
    "homepage": "https://github.com/cmstead/tdd-workshop#readme",
    "devDependencies": {
        "@types/chai": "^4.2.9",
        "@types/mocha": "^7.0.1",
        "chai": "^4.2.0",
        "mocha": "^7.0.1",
        "nanocommit": "^1.14.0",
        "ts-mocha": "^6.0.0"
    },
    "dependencies": {
        "typescript": "^3.7.5"
